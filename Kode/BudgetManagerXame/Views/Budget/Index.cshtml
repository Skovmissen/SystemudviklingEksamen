@model BudgetManagerXame.Models.Budget
@{
    ViewBag.Title = "Budgets";
}

<h2>Budgetter for virksomheden med fiscal id: @ViewBag.ID</h2>
<h4>@ViewBag.Token</h4>


<div class="row">
    @Html.ActionLink("Opret Budget", "Create", "Budget")
    <table class="table">
        <tr>
            <th>
                <p>År</p>
            </th>
            <th>
                <p>Beskrivelse</p>
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model.BudgetList)
        {
            <tr>
                <td>@Html.ActionLink(item["Year"].ToString(), "Details", new { id = item["Id"] })</td>
                <td>@item["Description"]</td>
                <td>@Html.ActionLink("Inddatere", "Create", "Estimate", new { id = item["Id"] }) | @Html.ActionLink("Oversigt", "Details", new { id = item["Id"] }) | @Html.ActionLink("Redigere", "Edit", new { id = item["Id"] }) | @Html.ActionLink("Slet", "Delete", new { id = item["Id"] })  </td>
            </tr>
        }

    </table>



</div>







